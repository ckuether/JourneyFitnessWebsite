{
  "hosting": {
    "public": "dist/JourneyFitnessWebsite",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "appAssociation": "AUTO",
    "rewrites": [
      { "source": "!/links/**", "destination": "/index.html" },
      { "source": "/links/**", "dynamicLinks": true }
    ]
  }
}
